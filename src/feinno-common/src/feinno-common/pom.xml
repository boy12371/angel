			<artifactId>juds</artifactId>
			<version>0.94</version>
		</dependency>
		<dependency>
			<groupId>com.sun.tools</groupId>
			<artifactId>tools</artifactId>
			<version>1.7.0_04</version>
		</dependency>
		<dependency>
			<groupId>org.apache.tomcat</groupId>
			<artifactId>jdbc-pool</artifactId>
